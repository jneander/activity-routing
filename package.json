{
  "name": "@jneander/activity-routing",
  "version": "1.0.1",
  "description": "Routing by domain concepts instead of URLs",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p ./config/tsconfig.cjs.json",
    "build:esm": "tsc -p ./config/tsconfig.esm.json",
    "clean": "rimraf dist/*",
    "lint:eslint": "eslint .",
    "lint:eslint:fix": "eslint --fix .",
    "lint:prettier": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "prebuild": "npm run clean",
    "test": "mocha",
    "test:watch": "nodemon -e --exec 'npm run test'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jneander/activity-routing"
  },
  "keywords": [
    "routing"
  ],
  "author": "Jeremy Neander <jeremy.neander@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/jneander/activity-routing",
  "dependencies": {
    "qs": "^6.7.0",
    "url-pattern": "^1.0.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@jneander/prettier-config": "^1.0.0",
    "chai": "^4.3.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.8.1",
    "sinon": "^15.0.0",
    "typescript": "^4.9.4"
  },
  "prettier": "@jneander/prettier-config"
}
